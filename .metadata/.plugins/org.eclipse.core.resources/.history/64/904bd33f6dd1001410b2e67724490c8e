package uefs.redes.model;

import java.util.ArrayList;

public class MovieInformation {
	
	private static final long serialVersionUID = 1L;
	private String name_file = null; 
	private ArrayList< String > tags_file = new ArrayList< String >();
	private String description = null;
	private int quantity = 0;
	private String category = null;
	
	
	public MovieInformation( String name, ArrayList< String > tags, String description,int quantity,String category )
	{
		this.name_file = name;
		this.tags_file = tags;
		this.setDescricao(description);
		this.quantity = quantity;
		this.setCategoria(category);
	}
	public MovieInformation( String name, String description,int quantity,String category )
	{
		this.name_file = name;
		this.setDescricao(description);
		this.quantity = quantity;
		this.setCategoria(category);
	}
	public String getNome_arquivo() {
		return name_file;
	}
	public void setNome_arquivo(String nome_arquivo) {
		this.name_file = nome_arquivo;
	}
	public ArrayList< String > getTags_arquivo() {
		return tags_file;
	}
	public void setTags_arquivo(ArrayList< String > tags_arquivo) {
		this.tags_file = tags_arquivo;
	}
	public int getQualidade() {
		return quantity;
	}
	public void setQualidade(int qualidade) {
		this.quantity = qualidade;
	}
	public String getDescricao() {
		return description;
	}
	public void setDescricao(String descricao) {
		this.description = descricao;
	}
	public String getCategoria() {
		return category;
	}
	public void setCategoria(String categoria) {
		this.category = categoria;
	}

}
